type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  children: [Child!]!
}

type Child {
  id: ID! @id
  name: String!
  parent: User!
  logEntries: [LogEntry!]!
}

type LogEntry {
  id: ID! @id
  child: Child!
  note: String!
  createdBy: User!
  createdAt: DateTime! @createdAt
}


