type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  children: [Child!]!
  logEntries: [LogEntry!]!
}

type Child {
  id: ID! @id
  name: String!
  parent: User!
}

type LogEntry {
  id: ID! @id
  createdAt: DateTime! @createdAt
  note: String!
  createdBy: User
}


